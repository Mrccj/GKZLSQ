/* pages/index/index.wxss */

page {
  width: 100%;
  height: 100%;
  background: #b1ffd8;
}

.boxTop {
  width: 100%;
  height: 500rpx;
  background: #6be493;
  /*border-radius: 0 0 10px 10px;*/
  border-bottom-left-radius: 15px 5px;
  border-bottom-right-radius: 15px 5px;
  /*transform:skew(45deg);*/
  position: relative;
  z-index: 3;
}

.round {
  width: 140rpx;
  height: 140rpx;
  background: #4da65a;
  border: 10px solid #6be493;
  border-radius: 50%;
  position: absolute;
  bottom: -20%;
  left: calc(50% - 80rpx);
  z-index: 6;
  text-align: center;
  line-height: 140rpx;
  font-size: 24rpx;
  color: #fff;
}

.roundButton{
  width: 140rpx;
  white-space: nowrap;
  height: 140rpx;
  background: #4da65a;
  border: 10px solid #6be493;
  border-radius: 50%;
  position: absolute;
  bottom: -20%;
  left: calc(50% - 80rpx);
  z-index: 6;
  text-align: center;
  line-height: 140rpx;
  font-size: 24rpx;
  color: #fff;
  padding: 0;
  box-sizing:unset;
}

.boxTop::before {
  display: block;
  content: "";
  position: absolute;
  width: calc(45%);
  height: 106rpx;
  bottom: -21%;
  background: #fff;
  left: 5%;
  border-top-right-radius: 70% 50px;
  z-index: 2;
}

.boxTop::after {
  display: block;
  content: "";
  position: absolute;
  width: calc(45%);
  height: 106rpx;
  bottom: -21%;
  background: #fff;
  right: 5%;
  /* border-top-left-radius: 55% 50px; */
  border-top-left-radius: 70% 50px;
  z-index: 2;
}

.boxBottom {
  width: 90%;
  height: 500rpx;
  background: #fff;
  margin: 0 auto;
  border-bottom-left-radius: 32rpx;
  border-bottom-right-radius: 32rpx;
  position: absolute;
  z-index: 1;
  top: 600rpx;
  left: 5%;
}

.ba_clock {
  width: 70%;
  height: 40rpx;
  background: #6be493;
  margin: 0 auto;
}

.div_one {
  width: 86%;
  height: 50rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 2%;
  align-items: center;
  left: 5%;
  position: absolute;
  z-index: 5;
}

.wenben {
  font-size: 24rpx;
  color: #555;
}

.vip{
  width: 686rpx;
  height: 240rpx;
  padding: 32rpx;
  
}
.vip_img{
